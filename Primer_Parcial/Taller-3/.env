# Variables de entorno para el Taller-3

# Puerto del servidor
PORT=3000

# Ruta de archivos públicos
PUBLIC_PATH=public

# Configuración del ORM
ORM_TYPE=typeorm

# Base de datos PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=123456
DB_NAME=taller3_db

# Base de datos SQLite (para Sequelize)
SQLITE_DB_PATH=./database.sqlite

# Configuración de aplicación
NODE_ENV=development
API_PREFIX=/api/v1
